{
    "name": "deepak/helper-laravel",
    "description": "A package to help developers with common tasks in Laravel.",
    "keywords": ["laravel", "helper", "utility", "tools"],
    "type": "library",
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "autoload": {
    "psr-4": {
        "DeepakDev\\LaravelHelper\\": "src/"
    }
},
"extra": {
    "laravel": {
        "providers": [
            "DeepakDev\\LaravelHelper\\LaravelHelperServiceProvider"
        ],
        "aliases": {
            "LaravelHelper": "DeepakDev\\LaravelHelper\\Facades\\LaravelHelperFacade"
        }
    }
},

    "license": "MIT",
    "authors": [
        {
            "name": "Deepak",
            "email": "dk43it@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
